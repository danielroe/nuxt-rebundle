{
  "name": "nuxt-rebundle",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Bundler optimisations for Nuxt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielroe/nuxt-rebundle.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "performance",
    "lighthouse",
    "bundling"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "pnpm nuxt-module-build build --stub && pnpm nuxt-module-build prepare && nuxt prepare playground",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "true || vitest run",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.0",
    "acorn": "^8.13.0",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.12",
    "mlly": "^1.7.2",
    "pathe": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "1.11.0",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.2.1",
    "@nuxt/test-utils": "3.21.0",
    "@types/node": "24.10.2",
    "@vitest/coverage-v8": "4.0.15",
    "@vue/test-utils": "2.4.6",
    "bumpp": "10.3.2",
    "eslint": "9.39.1",
    "nuxt": "4.2.1",
    "typescript": "5.9.3",
    "vite": "7.2.7",
    "vitest": "4.0.15",
    "vitest-environment-nuxt": "1.0.1",
    "vue": "3.5.25",
    "vue-tsc": "3.1.8"
  },
  "peerDependencies": {
    "nuxt": ">=3.12.0"
  },
  "resolutions": {
    "@nuxt/kit": "^4.0.0",
    "nuxt-rebundle": "link:.",
    "vue": "3.5.25"
  },
  "packageManager": "pnpm@10.25.0"
}
