{
  "name": "nuxt-rebundle",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Bundler optimisations for Nuxt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielroe/nuxt-rebundle.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "performance",
    "lighthouse",
    "bundling"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "pnpm nuxt-module-build build --stub && pnpm nuxt-module-build prepare && nuxt prepare playground",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "true || vitest run",
    "test:types": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "acorn": "^8.13.0",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.12",
    "mlly": "^1.7.2",
    "pathe": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "1.4.1",
    "@nuxt/module-builder": "1.0.1",
    "@nuxt/schema": "3.17.5",
    "@nuxt/test-utils": "3.19.1",
    "@types/node": "22.15.30",
    "@vitest/coverage-v8": "3.2.3",
    "@vue/test-utils": "2.4.6",
    "bumpp": "10.1.1",
    "eslint": "9.28.0",
    "nuxt": "3.17.5",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.3",
    "vitest-environment-nuxt": "1.0.1",
    "vue": "3.5.16",
    "vue-tsc": "2.2.10"
  },
  "peerDependencies": {
    "nuxt": ">=3.12.0"
  },
  "resolutions": {
    "@nuxt/kit": "3.17.5",
    "nuxt-rebundle": "link:.",
    "vue": "3.5.16"
  },
  "packageManager": "pnpm@10.12.1"
}
